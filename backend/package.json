{
  "name": "shumilog-backend",
  "version": "1.0.0",
  "description": "Backend API for Hobby Content Log Service",
  "main": "src/index.ts",
  "scripts": {
    "dev": "npm run dev:server",
    "dev:server": "nodemon",
    "dev:worker": "wrangler dev --local --persist-to=.wrangler/state",
    "build": "tsc --noEmit",
    "deploy": "wrangler deploy",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:contract": "vitest run tests/contract",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "d1:migrate": "yes | NO_D1_WARNING=true wrangler d1 migrations apply shumilog-db-dev --local",
    "d1:seed": "NO_D1_WARNING=true wrangler d1 execute shumilog-db-dev --local --file src/db/seeds.sql",
    "db:migrate": "tsx src/db/migrate.ts --no-seed",
    "db:seed": "tsx src/db/migrate.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.2.0",
    "hono": "^4.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240222.0",
    "@types/node": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "miniflare": "^4.20250924.0",
    "@vitest/coverage-v8": "^0.34.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.2.0",
    "vitest": "^0.34.0",
    "wrangler": "^4.40.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
