{
  "name": "shumilog-backend",
  "version": "1.0.0",
  "description": "Backend API for Hobby Content Log Service",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev --local",
    "build": "tsc --noEmit",
    "deploy": "wrangler deploy",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:contract": "vitest run tests/contract",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "db:migrate": "NO_D1_WARNING=true wrangler d1 migrations apply shumilog-db-dev --local",
    "db:seed": "NO_D1_WARNING=true wrangler d1 execute shumilog-db-dev --local --file src/db/seeds.sql",
    "dev:create-session": "tsx src/scripts/create-session.ts"
  },
  "dependencies": {
    "hono": "^4.9.9",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250927.0",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "miniflare": "^4.20250924.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.40.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
