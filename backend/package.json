{
  "name": "shumilog-backend",
  "version": "1.0.0",
  "description": "Backend API for Hobby Content Log Service",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev --local",
    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "deploy": "wrangler deploy",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:contract": "vitest run tests/contract",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "db:migrate": "NO_D1_WARNING=true wrangler d1 migrations apply shumilog-db-dev --local",
    "db:seed": "NO_D1_WARNING=true wrangler d1 execute shumilog-db-dev --local --file src/db/seeds.sql",
    "dev:create-session": "tsx src/scripts/create-session.ts"
  },
  "dependencies": {
    "@mixmark-io/domino": "^2.2.0",
    "@types/turndown": "^5.0.5",
    "hono": "^4.9.10",
    "turndown": "^7.2.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251004.0",
    "@types/node": "^22.18.8",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest-openapi": "^0.14.2",
    "miniflare": "^4.20251001.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.42.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
