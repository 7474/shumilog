# Base URL for API requests during development and testing
VITE_API_BASE_URL=http://localhost:8787/api
# Optional override for Vite dev server proxy target
VITE_DEV_API_PROXY=http://localhost:8787
# Frontend URL for Cloudflare Image Resizing (defaults to window.location.origin if not set)
# In production, set this to your frontend domain (e.g., https://shumilog.dev)
VITE_FRONTEND_URL=http://localhost:5173

# Advertising Configuration
# Set to 'true' to enable advertisements on the site
VITE_ADS_ENABLED=false
# Google AdSense Client ID (e.g., ca-pub-1234567890123456)
VITE_ADSENSE_CLIENT_ID=
